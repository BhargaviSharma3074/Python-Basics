#use of arithmatic operators
a = int(input("Enter the value in a: "))
b = int(input("Enter the value in b: "))

c = a+b
d = a-b
e = a*b
f = a/b
g = a//b
h = a%b
i = a**3

print("a+b = ",c)
print()#empty print function
print("a-b = ",d)
print("a*b = ",e)
print("a/b = ",f)
print("a//b = ",g)
print("a%b = ",h)
print("a**3 = ",i)
